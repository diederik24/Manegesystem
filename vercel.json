{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "api/get-customer-data.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/get-customer-data",
      "dest": "/api/get-customer-data.ts"
    },
    {
      "src": "/((?!api).*)",
      "dest": "/index.html"
    }
  ]
}
